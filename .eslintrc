{
  "extends": [
    "algolia",
    "algolia/jest",
    "algolia/typescript"
  ],
  "rules": {
    "no-continue": "off",
    "valid-jsdoc": "off",
    "import/extensions": [
      "error",
      {
        "ignorePackages": true,
        "pattern": {
          "js": "always",
          "ts": "never"
        }
      }
    ]
  },
  "overrides": [
    {
      "files": [
        "**/*.ts"
      ],
      "rules": {
        "consistent-return": [
          "off"
        ],
        "no-dupe-class-members": [
          "off"
        ],
      }
    }
  ]
}
