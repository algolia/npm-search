{
  "extends": [
    "algolia",
    "algolia/jest",
    "algolia/typescript"
  ],
  "rules": {
    "valid-jsdoc": "off",
    "import/extensions": [
      "error",
      {
        "ignorePackages": true,
        "pattern": {
          "js": "always",
          "ts": "never"
        }
      }
    ],
    // temp during mig
    "jsdoc/require-param-description": "off",
    "jsdoc/require-description": "off",
    "jsdoc/no-undefined-types": "off",
  }
}
