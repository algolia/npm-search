{
  "name": "npm-search",
  "version": "1.0.0",
  "private": true,
  "author": {
    "name": "Algolia, Inc.",
    "url": "https://www.algolia.com"
  },
  "scripts": {
    "dev": "babel-node index.js",
    "start": "node lib/index.js",
    "install": "npm run build",
    "build": "babel *.js -d lib",
    "test": "eslint .",
    "format": "prettier --write *.{js,md,json}",
    "doctoc": "doctoc --notitle --maxlevel 3 README.md",
