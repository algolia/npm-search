{
  "name": "npm-search",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "author": {
    "name": "Algolia, Inc.",
    "url": "https://www.algolia.com"
  },
  "scripts": {
    "start": "node --max-old-space-size=920 --experimental-modules src/index.js",
    "test": "jest --forceExit && eslint .",
    "test:watch": "jest --watchAll --no-watchman",
    "lint": "eslint --ext=jsx,ts,tsx,js .",
    "doctoc": "doctoc --notitle --maxlevel 3 README.md"
  },
  "lint-staged": {
    "*.js": [
      "yarn lint --fix",
      "git add"
    ]
  },
  "renovate": {
    "extends": [
      "config:js-app",
      "algolia"
    ]
  },
  "license": "MIT",
  "dependencies": {
    "@sentry/node": "5.30.0",
    "algoliasearch": "3.35.1",
    "async": "3.2.0",
    "babel-cli": "6.26.0",
    "babel-preset-env": "1.7.0",
    "babel-preset-stage-2": "6.24.1",
    "bunyan": "1.8.15",
    "bunyan-debug-stream": "2.0.0",
    "dotenv": "8.2.0",
    "dtrace-provider": "0.8.8",
    "escape-html": "1.0.3",
    "got": "9.6.0",
    "gravatar-url": "3.1.0",
    "hosted-git-info": "2.7.1",
    "hot-shots": "6.8.7",
    "lodash": "4.17.21",
    "ms": "2.1.3",
    "nano": "8.2.3",
    "nice-package": "3.1.2",
    "numeral": "2.0.6",
    "object-sizeof": "1.6.1",
    "promise-rat-race": "1.5.1",
    "traverse": "0.6.6",
    "truncate-utf8-bytes": "1.0.2"
  },
  "devDependencies": {
    "@babel/core": "7.13.10",
    "@babel/preset-env": "7.13.10",
    "babel-eslint": "10.1.0",
    "babel-jest": "24.9.0",
    "doctoc": "1.4.0",
    "eslint": "7.27.0",
    "eslint-config-algolia": "18.0.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-eslint-comments": "3.2.0",
    "eslint-plugin-import": "2.23.4",
    "eslint-plugin-jasmine": "4.1.2",
    "eslint-plugin-jest": "24.3.6",
    "eslint-plugin-jsdoc": "35.1.2",
    "eslint-plugin-prettier": "3.4.0",
    "eslint-plugin-react": "7.24.0",
    "husky": "6.0.0",
    "jest": "27.0.4",
    "lint-staged": "11.0.0",
    "pre-commit": "1.2.2",
    "prettier": "2.3.0",
    "renovate-config-algolia": "2.1.3",
    "validator": "11.1.0"
  },
  "engines": {
    "node": "14.16.1",
    "yarn": "^1.22.0"
  },
  "jest": {
    "testEnvironment": "node",
    "testPathIgnorePatterns": [
      "node_modules",
      "lib"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
