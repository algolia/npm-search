  },
  "lint-staged": {
    "*.js": ["prettier --write", "git add"]
  },
  "license": "MIT",
  "dependencies": {
    "algoliasearch": "^3.22.1",
    "async": "^2.4.0",
    "babel-cli": "^6.24.1",
    "babel-preset-env": "^1.4.0",
    "babel-preset-stage-2": "^6.24.1",
    "bunyan": "^1.8.10",
    "bunyan-debug-stream": "^1.0.7",
    "dotenv": "^4.0.0",
    "escape-html": "^1.0.3",
    "got": "^8.0.0",
    "gravatar-url": "^2.0.0",
    "lodash": "^4.17.4",
    "ms": "^2.0.0",
    "nice-package": "^3.0.1",
    "numeral": "^2.0.6",
    "object-sizeof": "^1.2.0",
    "pouchdb-http": "^6.0.2",
    "promise-rat-race": "^1.5.1",
    "traverse": "^0.6.6",
    "truncate-utf8-bytes": "^1.0.2"
  },
  "devDependencies": {
    "babel-eslint": "8.0.3",
    "doctoc": "1.3.0",
    "eslint": "4.12.1",
    "eslint-config-algolia": "13.0.1",
    "eslint-config-prettier": "2.9.0",
    "eslint-import-resolver-webpack": "0.8.3",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-jasmine": "2.9.1",
    "eslint-plugin-jest": "21.4.1",
    "eslint-plugin-prettier": "2.3.1",
    "eslint-plugin-react": "7.5.1",
    "husky": "0.14.3",
    "lint-staged": "6.0.0",
    "pre-commit": "1.2.2",
    "prettier": "1.9.1"
  },
  "engines": {
    "node": ">=7"
  }
}
