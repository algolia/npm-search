{
  "name": "npm-search",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "author": {
    "name": "Algolia, Inc.",
    "url": "https://www.algolia.com"
  },
  "scripts": {
    "build:hot": "tsc -w --preserveWatchOutput",
    "build": "tsc -b",
    "clean": "rm -rf dist/",
    "dev": "",
    "doctoc": "doctoc --notitle --maxlevel 3 README.md",
    "lint": "eslint --ext=jsx,ts,tsx,js .",
    "prepare": "husky install",
    "start": "UV_THREADPOOL_SIZE=12 node  -r dotenv/config  --async-stack-traces --max-old-space-size=920 dist/index.js",
    "test:watch": "jest --watchAll --no-watchman",
    "test": "jest --forceExit"
  },
  "renovate": {
    "extends": [
      "config:js-app",
      "algolia"
    ]
  },
  "license": "MIT",
  "dependencies": {
    "@sentry/node": "6.5.1",
    "algoliasearch": "4.9.1",
    "async": "3.2.0",
    "bunyan": "1.8.15",
    "bunyan-debug-stream": "2.0.0",
    "dotenv": "10.0.0",
    "dtrace-provider": "0.8.8",
    "escape-html": "1.0.3",
    "got": "11.8.2",
    "gravatar-url": "3.1.0",
    "hosted-git-info": "2.7.1",
    "hot-shots": "8.3.2",
    "lodash": "4.17.21",
    "ms": "2.1.3",
    "nano": "8.2.3",
    "nice-package": "3.1.2",
    "numeral": "2.0.6",
    "object-sizeof": "1.6.1",
    "promise-rat-race": "1.5.1",
    "traverse": "0.6.6",
    "truncate-utf8-bytes": "1.0.2"
  },
  "devDependencies": {
    "@types/async": "3.2.6",
    "@types/escape-html": "1.0.1",
    "@types/hosted-git-info": "3.0.1",
    "@types/jest": "26.0.23",
    "@types/lodash": "4.14.170",
    "@types/ms": "0.7.31",
    "@types/numeral": "2.0.1",
    "@types/traverse": "0.6.32",
    "@types/truncate-utf8-bytes": "1.0.0",
    "@typescript-eslint/eslint-plugin": "4.26.0",
    "@typescript-eslint/parser": "4.26.0",
    "doctoc": "2.0.0",
    "eslint": "7.27.0",
    "eslint-config-algolia": "18.0.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-eslint-comments": "3.2.0",
    "eslint-plugin-import": "2.23.4",
    "eslint-plugin-jasmine": "4.1.2",
    "eslint-plugin-jest": "24.3.6",
    "eslint-plugin-jsdoc": "35.1.2",
    "eslint-plugin-prettier": "3.4.0",
    "eslint-plugin-react": "7.24.0",
    "husky": "6.0.0",
    "jest": "27.0.4",
    "jest-junit": "12.1.0",
    "lint-staged": "11.0.0",
    "pre-commit": "1.2.2",
    "prettier": "2.3.0",
    "renovate-config-algolia": "2.1.3",
    "ts-jest": "27.0.2",
    "typescript": "4.3.2",
    "validator": "13.6.0"
  },
  "engines": {
    "node": "14.16.1",
    "yarn": "^1.22.0"
  },
  "jest": {
    "testEnvironment": "node",
    "testPathIgnorePatterns": [
      "node_modules",
      "lib"
    ]
  }
}
